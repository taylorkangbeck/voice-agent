{
  "name": "voice-agent",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "export": "tsc",
    "start:build": "tsc",
    "start:run": "node build/index.js",
    "start": "dotenvx run -- concurrently npm:start:*",
    "dev:build": "tsc -w",
    "dev:run": "nodemon -r dotenv/config build/index.js",
    "dev": "concurrently npm:dev:*",
    "test": "jest",
    "init-flows": "dotenvx run -- ts-node src/scripts/initFlows.ts",
    "init-actions": "dotenvx run -- ts-node src/scripts/initActions.ts",
    "init-all": "dotenvx run -- npm run init-actions && npm run init-flows",
    "update-status": "dotenvx run -- ts-node src/scripts/updateStatus.ts",
    "tunnel": "ngrok http --domain=${BASE_DOMAIN} http://localhost:3000"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@google/genai": "^0.7.0",
    "@langchain/community": "^0.3.39",
    "@langchain/core": "^0.3.43",
    "@langchain/langgraph": "^0.2.62",
    "@langchain/openai": "^0.5.2",
    "@notionhq/client": "^2.3.0",
    "@types/express": "^4.17.21",
    "@types/express-ws": "^3.0.4",
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.1",
    "express": "^4.19.2",
    "express-ws": "^5.0.2",
    "langchain": "^0.3.20",
    "neo4j-driver": "^5.28.1",
    "ts-json-schema-generator": "^2.3.0",
    "twilio": "^5.5.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.16",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "latest"
  }
}
